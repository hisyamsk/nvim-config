{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "typescript.tsx",
    "graphql",
    "html",
    "vue",
    "svelte"
  ],
  "explorer.width": 30,
  "explorer.icon.enableNerdfont": true,
  "prettier.disableLanguages": ["cmake"],
  "coc.preferences.formatOnType": true
}
